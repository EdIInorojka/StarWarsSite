<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StarWarsCharacters</title>
    <link rel="shortcut icon" type="image/ico" href="~/images/favicon.ico">
    <link rel="stylesheet" href="css/normalizecss.css">
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/media.css" />
</head>
<body>
    <header>
        <div class="container">
            <nav class="header-nav">
                <a class="" asp-area="" asp-controller="Home" asp-action="Index">StarWarsCharacters</a>
                <ul class="nav-ul">
                    <li class="">
                        <a class="" asp-area="" asp-controller="Home" asp-action="Index">Главная</a>
                    </li>
                    <li class="">
                        <a class="" asp-area="" asp-controller="Home" asp-action="AddCharacter">Добавить персонажа</a>
                    </li>
                </ul>
            </nav>
        </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer>
        <div class="container">
            <p class="footer-p">&copy; 2023 - StarWarsSite - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></p>
        </div>
    </footer>
    @await RenderSectionAsync("Scripts", required: false)
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>
</body>
</html>
